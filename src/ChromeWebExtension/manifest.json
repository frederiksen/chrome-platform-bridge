{
  "manifest_version": 2,
  "name": "Chrome Platform Bridge",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.0.1",
  "description": "Chrome Platform Bridge",
  "content_scripts": [
    {
      "js": [ "content-script.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "nativeMessaging"
  ]
}